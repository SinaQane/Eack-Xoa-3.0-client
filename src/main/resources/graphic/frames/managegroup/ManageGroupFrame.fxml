<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="380.0" prefWidth="600.0" style="-fx-background-color: #1E2425;" xmlns="http://javafx.com/javafx/16"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.frames.managegroup.ManageGroupFrameFXML">
    <TextField fx:id="titleTextField" alignment="CENTER" layoutX="199.0" layoutY="68.0" prefHeight="24.0"
               prefWidth="203.0" promptText="Group Title"/>
    <TextField fx:id="removedTextField" alignment="CENTER" layoutX="162.0" layoutY="227.0" prefHeight="24.0"
               prefWidth="277.0" promptText="Users to remove (separated by space)"/>
    <TextField fx:id="addedTextField" alignment="CENTER" layoutX="163.0" layoutY="166.0" prefHeight="24.0"
               prefWidth="277.0" promptText="Users to add (separated by space)"/>
    <Button fx:id="doneButton" layoutX="253.0" layoutY="301.0" mnemonicParsing="false" onAction="#done"
            prefHeight="24.0" prefWidth="96.0" text="Create/Edit"/>
</Pane>
